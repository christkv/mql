        var foldData = this.$foldData;
        var foldRow  = null;
        var foldLine;
        var fold;
        var argsFold;
        var folds;
        var added = false;

        if (placeholder instanceof Fold) {
            argsFold = placeholder;
            startRow = argsFold.range;
            placeholder = argsFold.placeholder;
        }

